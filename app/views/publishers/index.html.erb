<%= render 'shared/result_messages', record: @publisher %>

<h1>Publishers</h1>

<div id='publishers'>
  <% @publishers.each do |publisher| %>
    <%= render publisher %>
    <p>
      <%= link_to 'View', publisher %>
      <%= link_to 'Edit', edit_publisher_path(publisher) %>
    </p>
  <% end %>
</div>

<%= link_to 'New publisher', new_publisher_path %>
