<%= render 'shared/result_messages', record: @library %>

<h1>Libraries</h1>

<div id='libraries'>
  <% @libraries.each do |library| %>
    <%= render library %>
    <p>
      <%= link_to 'View', library %>
      <%= link_to 'Edit', edit_library_path(library) %>
    </p>
  <% end %>
</div>

<%= link_to 'New library', new_library_path %>
