<%= render 'shared/result_messages', record: @published_date %>

<h1>Published dates</h1>

<div id='published_dates'>
  <% @published_dates.each do |published_date| %>
    <%= render published_date %>
    <p>
      <%= link_to 'Show this published date', published_date %>
    </p>
  <% end %>
</div>

<%= link_to 'New published date', new_published_date_path %>
