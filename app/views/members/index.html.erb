<%= render 'shared/result_messages', record: @member %>

<h1>Members</h1>

<div id='members'>
  <% @members.each do |member| %>
    <%= render member %>
    <p>
      <%= link_to 'View', member %>
      <%= link_to 'Edit', edit_member_path(member) %>
    </p>
  <% end %>
</div>

<%= link_to 'New member', new_member_path %>
